# Managed by Puppet do not edit!

[Unit]
Description=<%= $description %>

[Timer]
<% if $on_calendar { -%>
OnCalendar=<%= $on_calendar %>
<% } -%>
<% if $on_boot { -%>
OnBootSec=<%= $on_boot %>
<% } -%>
<% if $on_unitactive { -%>
OnUnitActiveSec=<%= $on_unitactive %>
<% } -%>
<% unless $additional_params =~ Undef {
  $additional_params.each |$param| { -%>
<%= $param %>
<% } -%> 
<% } -%> 

[Install]
WantedBy=timers.target
